<view class="{{['userpost',AppStyle]}}"><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">图库</view><view data-event-opts="{{[['tap',[['toUrl',['https://www.pexels.com/']]]]]}}" class="action" bindtap="__e"><text class="text-blue">Pexels</text></view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="data-box"><view class="cu-bar bg-white search imagetoday-search"><view class="search-form round"><input type="text" placeholder="输入搜索关键字(暂只支持英文)" data-event-opts="{{[['input',[['__set_model',['','searchText','$event',[]]]]]]}}" value="{{searchText}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['searchTag']]]]}}" class="cuIcon-search" bindtap="__e"></text></view></view><block wx:if="{{$root.g0==0}}"><view class="no-data"><text class="cuIcon-text"></text>暂时没有数据</view></block><view class="ImageList"><block wx:for="{{ImageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="ImageList-box"><block wx:if="{{type=='post'}}"><view data-event-opts="{{[['tap',[['goPost',['$0'],[[['ImageList','',index,'src.large']]]]]]]}}" class="cu-btn bg-blue setImage" bindtap="__e">选择</view></block><image src="{{item.src.large2x}}" mode="widthFix" data-event-opts="{{[['tap',[['previewImage',['$0'],[[['ImageList','',index,'src.large2x']]]]]]]}}" bindtap="__e"></image><view class="image-info"><text data-event-opts="{{[['tap',[['toUrl',['$0'],[[['ImageList','',index,'photographer_url']]]]]]]}}" bindtap="__e">{{"@"+item.photographer}}</text></view></view></block></view><block wx:if="{{$root.g1>0}}"><view data-event-opts="{{[['tap',[['loadMore',['$event']]]]]}}" class="load-more" bindtap="__e"><text>{{moreText}}</text></view></block></view><block wx:if="{{isLoading==0}}"><view class="loading"><view class="loading-main"><image src="../../static/loading.gif"></image></view></view></block></view>